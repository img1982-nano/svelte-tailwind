{
  "name": "@jill64/svelte-tailwind",
  "description": "ðŸŒ€ Simplified TailwindCSS Configuration for Svelte",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-tailwind/issues",
  "homepage": "https://github.com/jill64/svelte-tailwind#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-tailwind.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "config",
    "sveltekit",
    "tailwind"
  ],
  "scripts": {
    "dev": "pnpm build:lib && cd demo && pnpm dev",
    "build": "pnpm build:lib && pnpm build:demo",
    "build:lib": "tsc && npx publint",
    "build:demo": "cd demo && pnpm build",
    "preview": "pnpm build:lib && cd demo && pnpm preview",
    "prepack": "pnpm build",
    "lint": "pnpm lint:lib && pnpm lint:demo",
    "lint:lib": "npx eslint .",
    "lint:demo": "cd demo && pnpm lint",
    "format": "pnpm format:lib && pnpm format:demo",
    "format:lib": "npx prettier -w .",
    "format:demo": "cd demo && pnpm format",
    "test": "playwright test"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.0.0",
    "@jill64/playwright-config": "1.0.0",
    "@jill64/prettier-config": "1.0.0",
    "@playwright/test": "1.40.1",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@jill64/tailwind-grid-auto": "1.3.0",
    "@jill64/tailwind-reactions": "1.2.0",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.32",
    "postcss-load-config": "5.0.2",
    "tailwindcss": "3.3.6"
  }
}